{
    "model_name": "LSTM_AE",
    "model_class": "LSTMAE",
    "autoencoder": true,
    "model_params": {
        "L": 1800,
        "n_channels": 8,
        "embedding_dim": 4,
        "num_layers": 5
    },
    "hyperparameters": {
        "max_epochs": 100,
        "lr": 0.001,
        "random_state": 218171,
        "optimizer": "Adam",
        "verbose": true,
        "batch_size": 64,
        "train_split": "TSValidSplit",
        "callbacks": [
            {
                "name": "EarlyStopping",
                "parameters": {
                    "patience": 15,
                    "load_best": true
                }
            },
            {
                "name": "GradientNormClipping",
                "parameters": {
                    "gradient_clip_value": 1.0
                }
            }
        ]
    }
}
